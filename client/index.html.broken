        }
        
        /* iOS Modal */
        .modal-content {
            background: #ffffff !important;
            border: none !important;
            border-radius: 14px !important;
            overflow: hidden;
            box-shadow: 0 10px 40px rgba(0, 0, 0, 0.2) !important;
        }
        
        .modal-header {
            border-bottom: 0.5px solid rgba(60, 60, 67, 0.12) !important;
            background: #f9f9fb;
            padding: 16px 20px;
        }
        
        .modal-title {
            color: #000000;
            font-weight: 600;
            font-size: 17px;
            text-align: center;
            letter-spacing: -0.4px;
        }
        
        .modal-body {
            color: #000000;
            padding: 20px;
            font-size: 17px;
            line-height: 1.4;
            letter-spacing: -0.4px;
        }
        
        .modal-footer {
            border-top: 0.5px solid rgba(60, 60, 67, 0.12) !important;
            background: #f9f9fb;
            padding: 0;
            display: flex;
        }
        
        .modal-footer .btn {
            flex: 1;
            border-radius: 0 !important;
            border-right: 0.5px solid rgba(60, 60, 67, 0.12) !important;
            background: transparent !important;
            color: #007aff !important;
            font-size: 17px !important;
            font-weight: 400 !important;
            padding: 16px !important;
            letter-spacing: -0.4px;
        }
        
        .modal-footer .btn:last-child {
            border-right: none !important;
            font-weight: 600 !important;
        }
        
        .modal-footer .btn:hover {
            background: #f2f2f7 !important;
        }
        
        .close {
            color: #8e8e93 !important;
            opacity: 1 !important;
            font-size: 28px;
            font-weight: 300;
            line-height: 0.7;
        }
        
        /* Connecting screen */
        #connecting > div {
            background: #ffffff !important;
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08) !important;
            border: none !important;
            border-radius: 14px !important;
            padding: 32px !important;
        }
        
        #connecting h2 {
            color: #000000;
            font-size: 22px;
            font-weight: 600;
            margin-bottom: 10px;
            letter-spacing: -0.4px;
        }
        
        #connecting p {
            color: #8e8e93;
            font-size: 15px;
            letter-spacing: -0.2px;
        }
        
        /* Remove bootstrap button styles */
        .btn:hover, .btn:focus, .btn:active, .btn:active:focus {
            box-shadow: none !important;
            outline: none !important;
        }
        
        /* Safari-specific CSS */
        @supports (-webkit-touch-callout: inherit) {
            body {
                height: 1vh !important;
            }
            
            #helloDialog {
                -webkit-transform: translateZ(0);
            }
        }
        
        /* Smooth animations */
        * {
            -webkit-tap-highlight-color: transparent;
        }
        
        /* iOS bounce effect for buttons */
        @keyframes buttonTap {
            0% { transform: scale(1); }
            50% { transform: scale(0.95); }
            100% { transform: scale(1); }
        }
        
        /* Game stats panel */
        .game-stats {
            position: fixed;
            top: 10px;
            left: 50%;
            transform: translateX(-50%);
            background: rgba(255, 255, 255, 0.95);
            backdrop-filter: blur(20px);
            -webkit-backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.8);
            border-radius: 14px;
            padding: 8px 20px;
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.08);
            display: flex;
            gap: 20px;
            font-size: 14px;
            font-weight: 500;
            color: #000000;
            letter-spacing: -0.3px;
        }
        
        .game-stats span {
            display: flex;
            align-items: center;
            gap: 5px;
        }
