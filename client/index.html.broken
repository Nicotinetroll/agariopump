// LIVE TIMER - Override everything and show real countdown
console.log("ðŸŽ¯ Starting LIVE TIMER override");

var gameState = {
    phase: 1,        // 0=countdown, 1=playing, 2=intermission
    timeLeft: 600,   // 10 minutes
    roundNum: 1,
    startTime: Date.now()
};

// Override any existing timer
window.liveTimerActive = true;

setInterval(function() {
    if (!window.liveTimerActive) return;
    
    var timerText = document.getElementById('timerText');
    var roundNumber = document.getElementById('roundNumber');
    
    if (timerText && roundNumber) {
        roundNumber.textContent = gameState.roundNum;
        
        switch (gameState.phase) {
            case 0: // Countdown
                timerText.textContent = "Starting in " + gameState.timeLeft + "s";
                break;
                
            case 1: // Playing
                var minutes = Math.floor(gameState.timeLeft / 60);
                var seconds = gameState.timeLeft % 60;
                timerText.textContent = minutes + ":" + (seconds < 10 ? "0" : "") + seconds;
                break;
                
            case 2: // Intermission
                timerText.textContent = "Intermission: " + gameState.timeLeft + "s";
                break;
        }
        
        // Countdown
        gameState.timeLeft--;
        
        // Phase transitions
        if (gameState.timeLeft <= 0) {
            if (gameState.phase === 1) {
                // Game ended, start intermission
                gameState.phase = 2;
                gameState.timeLeft = 30;
                console.log("ðŸ Round " + gameState.roundNum + " ended - Intermission started");
            } else if (gameState.phase === 2) {
                // Intermission ended, start new round
                gameState.phase = 1;
                gameState.timeLeft = 600; // 10 minutes
                gameState.roundNum++;
                console.log("ðŸŽ® Round " + gameState.roundNum + " started!");
            } else if (gameState.phase === 0) {
                // Countdown ended, start game
                gameState.phase = 1;
                gameState.timeLeft = 600;
                console.log("ðŸš€ Game started!");
            }
        }
        
        // Log every minute
        if (gameState.phase === 1 && gameState.timeLeft % 60 === 0) {
            console.log("â° " + Math.floor(gameState.timeLeft / 60) + " minutes left in round " + gameState.roundNum);
        }
    }
}, 1000);

