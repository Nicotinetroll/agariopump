        }
        
        /* Improved Canvas styling */
        #canvas {
            border-radius: 0;
            box-shadow: none;
            background: #ffffff;
        }
        
        /* iOS style Leaderboard */
        #leaderboard {
            position: fixed;
            top: 10px;
            right: 10px;
            background: rgba(255, 255, 255, 0.95) !important;
            backdrop-filter: blur(20px);
            -webkit-backdrop-filter: blur(20px);
            border: 1px solid rgba(255, 255, 255, 0.8);
            border-radius: 14px;
            padding: 12px;
