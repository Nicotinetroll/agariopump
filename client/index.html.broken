function tryDecodeMessage(view, packetId) {
    try {
        var offset = 0;
        
        // Skip packet ID
        offset += 1;
        
        // Different decoding strategies
        if (packetId === 240) {
            offset += 4; // Skip some bytes for packet 240
        }
        
        if (packetId === 99 || packetId === 64) {
            // Chat message format
            if (offset < view.byteLength) {
                var flags = view.getUint8(offset);
                offset += 1;
                console.log("Chat flags:", flags);
                
                if (flags & 2) offset += 4;
                if (flags & 4) offset += 8; 
                if (flags & 8) offset += 16;
            }
        }
        
        // Try to read string
        var msg = "";
        var attempts = 0;
        while (offset < view.byteLength && attempts < 100) {
            var charCode = view.getUint16(offset, true);
            offset += 2;
            if (charCode === 0) break;
            if (charCode > 0 && charCode < 65536) {
                msg += String.fromCharCode(charCode);
            }
            attempts++;
        }
        
        console.log("ðŸ”¤ Decoded message from packet", packetId + ":", JSON.stringify(msg));
        
        if (msg && msg.indexOf("TIME:") >= 0) {
            console.log("â° FOUND TIME MESSAGE:", msg);
            processTimeMessage(msg);
        }
        
    } catch (e) {
        console.log("âŒ Error decoding packet", packetId + ":", e.message);
    }
}

