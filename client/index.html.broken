// Hook into WebSocket to catch TIME messages
(function() {
    console.log("Setting up WebSocket TIME message interceptor...");
    
    // Override native WebSocket
    var OriginalWebSocket = window.WebSocket;
    window.WebSocket = function(url, protocols) {
        console.log("WebSocket created for:", url);
        var ws = new OriginalWebSocket(url, protocols);
        
        // Override onmessage
        var originalOnMessage = ws.onmessage;
        ws.onmessage = function(event) {
            // Call original handler
            if (originalOnMessage) {
                originalOnMessage.call(this, event);
            }
            
            // Check for TIME messages in binary data
            if (event.data instanceof ArrayBuffer) {
                var view = new DataView(event.data);
                var packetId = view.getUint8(0);
                
                // Check for chat message packet (ID 99 or similar)
                if (packetId === 99) {
                    try {
                        var offset = 1;
                        // Skip flags
                        var flags = view.getUint8(offset);
                        offset += 1;
                        if (flags & 2) { offset += 4; }
                        if (flags & 4) { offset += 8; }
                        if (flags & 8) { offset += 16; }
                        
                        // Read message
                        var msg = "";
                        while (offset < view.byteLength) {
                            var charCode = view.getUint16(offset, true);
                            offset += 2;
                            if (charCode == 0) break;
                            msg += String.fromCharCode(charCode);
                        }
                        
                        console.log("Chat message intercepted:", msg);
                        
                        if (msg.indexOf("TIME:") === 0) {
                            console.log("TIME message detected:", msg);
                            processTimeMessage(msg);
                        }
                    } catch (e) {
                        console.log("Error parsing message:", e);
                    }
                }
            }
        };
        
        return ws;
    };
    
    // Copy properties
    window.WebSocket.prototype = OriginalWebSocket.prototype;
    window.WebSocket.CONNECTING = OriginalWebSocket.CONNECTING;
    window.WebSocket.OPEN = OriginalWebSocket.OPEN;
    window.WebSocket.CLOSING = OriginalWebSocket.CLOSING;
    window.WebSocket.CLOSED = OriginalWebSocket.CLOSED;
})();

function processTimeMessage(msg) {
    console.log("Processing TIME message:", msg);
    var parts = msg.split(":");
    var phase = parseInt(parts[1]);
    var timeLeft = parseInt(parts[2]);
    var roundNum = parseInt(parts[3]);
    var winner = parts[4] || "";
    
    console.log("Timer data: phase=" + phase + ", time=" + timeLeft + ", round=" + roundNum + ", winner=" + winner);
    
    var timerText = document.getElementById('timerText');
    var roundNumber = document.getElementById('roundNumber');
    
    if (timerText && roundNumber) {
        roundNumber.textContent = roundNum;
        
        if (phase === 0) {
            timerText.textContent = "Starting in " + timeLeft + "s";
        } else if (phase === 1) {
            var m = Math.floor(timeLeft / 60);
            var s = timeLeft % 60;
            timerText.textContent = m + ":" + (s < 10 ? "0" : "") + s;
        } else if (phase === 2) {
            timerText.textContent = "Intermission: " + timeLeft + "s";
            if (winner) {
                timerText.textContent += " | Winner: " + winner;
            }
        }
        
        console.log("Timer updated successfully");
    }
}
