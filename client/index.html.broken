        offset += 1;
        
        // Skip flags (1 byte) 
        var flags = view.getUint8(offset);
        offset += 1;
        
        // Skip color bytes
        offset += 3;
        
        // Read sender name (skip it)
        while (offset < view.byteLength) {
            var charCode = view.getUint16(offset, true);
            offset += 2;
            if (charCode === 0) break;
